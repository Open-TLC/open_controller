    },
    "sig_outputs": {
        "type": "group",
        "trigger": "change",
        "controller_mapping_mode": "direct",
        "controller_map": {
            "1": "270"
        },
        "signal_mapping_mode": "direct",
        "signal_map": {
            "1": "1"
        }
    },
    "rad_outputs":{
        "type": "radar",
        "trigger": "update",
        "radars": ["int_270_1", "int_270_2", "int_270_3", "all_vehs"]
    }
},

