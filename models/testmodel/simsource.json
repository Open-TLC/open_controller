{
"simulation":{
	"notes": "These are parameters for running the simulation",
	"sumocfg": "models/testmodel.sumcfg",
	"mode": "nats",
	"nats": {
		"ip": "localhost",
		"port": 4222
	}
},

"outputs":{
    "det_outputs": {
        "type": "detector",
        "trigger": "change",
        "det_mapping_mode": "direct",
        "det_map": {
            "sumo_det1":"det1"
        },
